# Config file for EOS runs OPT parameters
##### NEED TO CHANGE ALL OF THIS 

[run]
name : dark_photon_fisher_forecast 
output_dir : /projectnb/darkcosmo/dark_photon_project/21cmfast_cache/

# redshifts
[redshifts]
min : 5.0
max : 35.0

# UserParam
[user_params]
HII_DIM : 128
BOX_LEN : 256
USE_FFTW_WISDOM : True
USE_INTERPOLATION_TABLES : True
FAST_FCOLL_TABLES : True
USE_RELATIVE_VELOCITIES : True
POWER_SPECTRUM : 5
KEEP_3D_VELOCITIES : True

# AstroParam
[astro_params]
ALPHA_ESC: -0.3
F_ESC10 : -1.35
ALPHA_STAR : 0.5 
F_STAR10 : -1.25 
t_STAR : 0.5
F_STAR7_MINI : -2.5 
ALPHA_STAR_MINI : 0.0 
F_ESC7_MINI : -1.35 
L_X : 40.5
NU_X_THRESH : 500.0 
A_LW : 2.0
N_RSD_STEPS : 5
EPSILON4 : 0


# FlagOptions
[flag_options]
INHOMO_RECO : True
USE_MASS_DEPENDENT_ZETA : True
USE_TS_FLUCT : True 
USE_MINI_HALOS : True
FIX_VCB_AVG : False
USE_ETHOS : False
FILTER : 0
APPLY_RSDS : False


# Parameters to vary in the Fisher analysis
[vary]
astro_params_vary :
    ALPHA_ESC
    F_ESC10
    ALPHA_STAR
    F_STAR10
    t_STAR
    F_STAR7_MINI
    ALPHA_STAR_MINI
    F_ESC7_MINI
    L_X
    NU_X_THRESH 
    A_LW 
    EPSILON4
    
    
